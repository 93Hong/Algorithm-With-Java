import java.io.*;
import java.util.*;

public class Main {
	static BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
	static int N;
	static char map[][];
	static int ans[][];
	static Queue<Integer> q;

	public static void main(String args[]) throws NumberFormatException, IOException {
		N = Integer.parseInt(br.readLine());
		map = new char[N][N];
		q = new LinkedList<>();
		ans = new int[N][N];

		for (int i = 0; i < N; i++) {
			map[i] = br.readLine().toCharArray();
		}

		for (int i = 0; i < N; i++) {
			for (int j = 0; j < N; j++) {
				if (map[i][j] == '1')
					solve(i, j);
			}
			//solve(i);
		}

		for (int i = 0; i < N; i++) {
			for (int j = 0; j < N; j++) {
				if (map[i][j] == '1')
					solve(i, j);
			}
			//solve(i);
		}
		
	}

	public static void solve(int y, int x) {
		if (ans[y][x] == 1)
			return;
		
		ans[y][x] = 1;
		for (int i=0; i<N; i++) {
			if (map[x][i] == '1') {
				solve(x, i);
			}
		}
		
	}
}
