import java.io.*;
import java.util.*;

public class Main {
	static BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
	static int N;
	static char map[][];
	static int ans[][];
	static Queue<Integer> q;

	public static void main(String args[]) throws NumberFormatException, IOException {
		N = Integer.parseInt(br.readLine());
		map = new char[N][N];
		q = new LinkedList<>();
		ans = new int[N][N];

		for (int i = 0; i < N; i++) {
			String tmp = br.readLine();
			
		}

		for (int i = 0; i < N; i++) {
			for (int j = 0; j < N; j++) {
				System.out.print(map[i][j]);
			}
			System.out.println();
		}
		
		for (int i = 0; i < N; i++) {
			for (int j = 0; j < N; j++) {
				if (map[i][j] == '1')
					solve(i, j);
			}
		}

		for (int i = 0; i < N; i++) {
			for (int j = 0; j < N; j++) {
				System.out.print(ans[i][j]);
			}
			System.out.println();
		}

	}

	public static void solve(int y, int x) {
		System.out.println(y + " " + x);
		if (ans[y][x] == 1)
			return;

		ans[y][x] = 1;
		for (int i = 0; i < N; i++) {
			if (map[x][i] == '1') {
				solve(x, i);
			}
		}

	}
}
